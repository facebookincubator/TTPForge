{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "go",
      "request": "launch",
      "name": "Debug variadic params TTP",
      "mode": "debug",
      "program": "${workspaceRoot}",
      "args": [
        "-c",
        "config.yaml",
        "run",
        "ttps/examples/variadic-params/variadicParameterExample.yaml"
      ]
    },
    {
      "name": "Test Current File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}/${relativeFileDirname}",
      "showLog": true
    }
  ],
  "compounds": [
    {
      "name": "Debug variadic params TTP",
      "configurations": ["Debug variadic params TTP"],
      "stopAll": true
    }
  ]
}
